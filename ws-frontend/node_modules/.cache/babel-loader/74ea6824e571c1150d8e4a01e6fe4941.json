.getItem(\"loggedUser\"));\n      console.log(this.OceniProdavca);\n      console.log(logovaniKorisnik);\n      var requestData = {\n        oceniProdavcaDto: this.OceniProdavca,\n        logovani: JSON.parse(localStorage.getItem(\"loggedUser\"))\n      };\n      axios.post(\"http://localhost:8081/api/oceni-prodavca\", requestData, {\n        withCredentials: true\n      }).then(function (response) {\n        alert(response.data);\n      })[\"catch\"](function (err) {\n        console.log(err);\n        alert(err.response.data);\n      });\n    },\n    prijavi: function prijavi() {\n      if (localStorage.getItem(\"loggedUser\") != null) {\n        axios.post(\"http://localhost:8081/api/reportseller/\" + this.$route.params.id + \"?loggedUsername=\" + JSON.parse(localStorage.getItem(\"loggedUser\")).korisnickoIme, this.PrijavaDto, {\n          withCredentials: true\n        }).then(function (response) {\n          alert(response.data);\n        })[\"catch\"](function (err) {\n          console.log(err);\n          alert(err.response.data);\n        });\n      } else {\n        alert(\"Niste ulogovani!\");\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AA0GC,OAAOA,KAAP,MAAkB,OAAlB;AACA,eAAe;EACbC,IAAI,EAAE,iBADO;EAEbC,IAAI,EAAE,gBAAY;IAChB,OAAO;MACLC,OAAO,EAAE,EADJ;MAELC,QAAQ,EAAC,EAFJ;MAGLC,oBAAoB,EAAE,EAHjB;MAILC,aAAa,EAAC;QACbC,aAAa,EAAE;MADF,CAJT;MAOLC,UAAU,EAAE;IAPP,CAAP;EAUD,CAbY;EAcbC,OAAO,EAAE,mBAAY;IAAA;;IAGpBT,KAAI,CACDU,GADH,CACO,wCAAsC,KAAKC,MAAL,CAAYC,MAAZ,CAAmBC,EADhE,EACoE;MAC9DC,eAAe,EAAE;IAD6C,CADpE,EAIGC,IAJH,CAIQ,UAACC,QAAD,EAAc;MAClBC,OAAO,CAACC,GAAR,CAAY,UAAZ,EAAwBF,QAAQ,CAACd,IAAjC;MACA,KAAI,CAACC,OAAL,GAAea,QAAQ,CAACd,IAAxB;MAEAF,KAAI,CACCU,GADL,CACS,6CAA2C,KAAI,CAACP,OAAL,CAAagB,QAAb,CAAsBC,aAD1E,EACyF;QACjFN,eAAe,EAAE;MADgE,CADzF,EAIKC,IAJL,CAIU,UAACM,GAAD,EAAS;QAEX,KAAI,CAACjB,QAAL,GAAgBiB,GAAG,CAACnB,IAApB;QACAe,OAAO,CAACC,GAAR,CAAY,KAAI,CAACd,QAAjB;QAEA,KAAI,CAACC,oBAAL,GAA4B,KAAI,CAACD,QAAL,CAAckB,SAA1C;MAEH,CAXL,WAYW,UAACC,GAAD,